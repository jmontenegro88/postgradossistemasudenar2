<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="certificados" stopProcessing="true">
                    <match url="(.*)" />
                    <action type="Rewrite" url="22http://127.0.0.1:4000/{R:1}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
